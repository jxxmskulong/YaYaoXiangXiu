<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.yayao.bean.Category" table="category_tb" catalog="YaYaoXiangXiu_db">
        <id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="native" />
        </id>
        <property name="cateName" type="java.lang.String">
            <column name="CateName" length="40" />
        </property>
        <property name="cateDesc" type="java.lang.String">
            <column name="CateDesc" length="65535" />
        </property>
    </class>
</hibernate-mapping>
